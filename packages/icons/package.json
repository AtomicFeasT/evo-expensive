{
  "name": "@neo/icons",
  "version": "0.0.1",
  "description": "NeoExpertise's library.",
  "author": "Vitor Gouveia <vitorneves.gouveia10@gmail.com>",
  "repository": "https://github.com/Neo-Florescence/neo-expertise",
  "license": "MIT",
  "private": true,
  "main": "./dist/index.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "type": "./dist/types/index.d.ts",
      "import": "./dist/index.js"
    },
    "./lib": {
      "type": "./dist/types/lib/index.d.ts",
      "import": "./dist/lib/index.js"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/types/index.d.ts"
      ],
      "lib": [
        "./dist/types/lib/index.d.ts"
      ]
    }
  },
  "packageManager": "yarn@3.1.1",
  "scripts": {
    "dev": "rimraf dist && rollup -c -w",
    "build": "rimraf dist && rollup -c"
  },
  "dependencies": {
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@rollup/plugin-typescript": "^8.3.0",
    "@types/react": "17.0.39",
    "rimraf": "^3.0.2",
    "rollup": "^2.67.3",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-visualizer": "^5.5.4",
    "tslib": "^2.3.1",
    "typescript": "^4.3.5"
  },
  "devDependencies": {
    "react": "^17.0.2"
  },
  "peerDependencies": {
    "react": "^17.0.2"
  }
}
